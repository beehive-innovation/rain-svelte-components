		"@steeze-ui/heroicons": "^2.1.0",
		"@steeze-ui/svelte-icon": "^1.3.2",
		"@types/lodash": "^4.14.186",
		"abitype": "^0.1.7",
		"ajv": "^8.11.0",
		"codemirror": "^6.0.1",
		"ethers": "^5.7.1",
		"lodash-es": "^4.17.21",
